[MASTER]
; ignore=
; extension-pkg-whitelist=

[FORMAT]
# Maximum number of characters on a single line.
max-line-length=120

[BASIC]
; good-names=f, i
; generated-members=

[MESSAGES CONTROL]
disable=
    missing-function-docstring,
    missing-module-docstring,
    missing-class-docstring,
    consider-using-f-string,
    consider-using-max-builtin,

enable=
   useless-suppression,